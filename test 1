HOLD = false
end

function KeyDown(Key)
	if Key ~= "w" and Key ~= "a" and Key ~= "s" and Key ~= "d" then
		KEYHOLD = true
	if Key == "z" and ATTACK == false and Milk == false and Chill == false then
		Rifle_Bullet()()
	elseif Key == "z" and ATTACK == false and Milk == true then
		Float_Bullet()
	end

	if Key == "t" and ATTACK == false and Milk == false then
		Taunt()
	end

	if Key == "p" and ATTACK == false and Milk == false then
		TakeOnMe()
	end
	
	if Key == "e" and ATTACK == false and Milk == false and Chill == false then
Sky0.SkyboxBk = "rbxassetid://58372692"
Sky0.SkyboxDn = "rbxassetid://58372720"
Sky0.SkyboxFt = "rbxassetid://58372743"
Sky0.SkyboxLf = "rbxassetid://58372775"
Sky0.SkyboxRt = "rbxassetid://58372795"
Sky0.SkyboxUp = "rbxassetid://58372811"
    Speed = 16
	WACKYEFFECT({Time = 30, EffectType = "Sphere", Size = VT(20,20,20), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 0})
	VIS = 1305251774
	VOLUME = 1.5
	PITCH = 1.04
	sick.TimePosition = 0
	Mode = "Main"
		Gun.Parent = nil
		Chill = true
	elseif Key == "e" and ATTACK == false and Boost == false and Milk == false and Chill == true then
        Mode = "idk"
		CreamForm2()
	end
	
	if Key == "z" and ATTACK == false and Chill == true and Mode ~= "Main" then
    game.Lighting.TimeOfDay = "12:00:00"
    Speed = 16
	VIS = 1305251774
	VOLUME = 1.5
	PITCH = 1.04
	sick.TimePosition = 0
	Mode = "Main"
	end
	
	if Key == "x" and ATTACK == false and Chill == true and Mode ~= "Lucid" then
    game.Lighting.TimeOfDay = "18:30:00"
    Speed = 18
	VIS = 892260150
	VOLUME = 1.5
	PITCH = 1.02
	sick.TimePosition = 3
	Mode = "Lucid"
	end
	
	if Key == "q" and ATTACK == false and Milk == false and Chill == false then
		MilkForm()
	elseif Key == "q" and ATTACK == false and Boost == false and Milk == true then
		CreamForm()
	end
	
	if Key == "c" and Cooldown1 == false and ATTACK == false and Milk == false and Chill == false then
		BulletRoller()
	elseif Key == "c" and Boost == false and Cooldown4 == false and ATTACK == false and Milk == true then
		Booster()
	end

	if Key == "v" and Cooldown2 == false and ATTACK == false and Milk == false and Chill == false then
		SHATTER()
	elseif Key == "v" and Cooldown3 == false and ATTACK == false and Milk == true then
		AURORA()
	end

	if Key == "b" and ATTACK == false then
		   chatfunc(UnBanishTaunts[MRANDOM(1,#UnBanishTaunts)])
			TOBANISH = {}
	end

	if Key == "x" and ATTACK == false and Milk == false and Chill == false then
		Cream_Strike_rlly()
	elseif Key == "x" and ATTACK == false and Milk == true then
		Ground_Strike()
	   end
	end
end

function KeyUp(Key)
	if Key ~= "w" and Key ~= "a" and Key ~= "s" and Key ~= "d" then
		KEYHOLD = false
	end
end

	Mouse.Button1Down:connect(function(NEWKEY)
		MouseDown(NEWKEY)
	end)
	Mouse.Button1Up:connect(function(NEWKEY)
		MouseUp(NEWKEY)
	end)
	Mouse.KeyDown:connect(function(NEWKEY)
		KeyDown(NEWKEY)
	end)
	Mouse.KeyUp:connect(function(NEWKEY)
		KeyUp(NEWKEY)
	end)

--//=================================\\
--\\=================================//


function unanchor()
	if UNANCHOR == true then
		g = Character:GetChildren()
		for i = 1, #g do
			if g[i].ClassName == "Part" then
				g[i].Anchored = false
			end
		end
	end
end


